<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Product List</title>
    <style>
        .product-item {
            border: 1px solid #ccc;
            padding: 10px;
            margin: 10px;
            display: inline-block;
            width: 200px;
        }
        .product-item img {
            max-width: 100%;
        }
    </style>
</head>
<body>
    <h1>Product List</h1>
    <div id="product-list">
        <% products.forEach(product => { %>
            <div class="product-item">
                <h3><%= product.title %></h3>
                <p>Price: <%= product.price %></p>
                <p>Availability: <%= product.availability %></p>
                <img src="https://books.toscrape.com/<%= product.imageURL %>" alt="<%= product.title %>">
            </div>
        <% }); %>
    </div>
</body>
</html>
